package com.HotPlace.vo;

// 갤러리 카드 게시판에서 보일 미디어 파일
public class GalleryVo {

	private Integer galleryNum; // 갤러리 번호
	private Integer cbNum; // 카드 보드 번호
	private String src; // 파일 src
	private String originSrc; // 파일 원본 src

	public GalleryVo() {}
	
	public GalleryVo(Integer galleryNum, Integer cbNum, String src, String originSrc) {
		this.galleryNum = galleryNum;
		this.cbNum = cbNum;
		this.src = src;
		this.originSrc = originSrc;
	}

	public Integer getGalleryNum() {
		return galleryNum;
	}

	public void setGalleryNum(Integer galleryNum) {
		this.galleryNum = galleryNum;
	}

	public Integer getCbNum() {
		return cbNum;
	}

	public void setCbNum(Integer cbNum) {
		this.cbNum = cbNum;
	}

	public String getSrc() {
		return src;
	}

	public void setSrc(String src) {
		this.src = src;
	}

	public String getOriginSrc() {
		return originSrc;
	}

	public void setOriginSrc(String originSrc) {
		this.originSrc = originSrc;
	}

}
