.bt {
	position: relative;
	width: 80px;
	height: 25px;
	font-size: 12px;
	color: black;
	background: white;
	cursor: pointer;
	margin: 5px;

}
.bt:hover{
	border:1px solid;
	color: red;
}
table {
	width: 700px; 
	border-collapse: collapse;
	font-size: 12px;
	line-height: 15px;
	
}
table td, th{
	border:  1px solid #90b4e63d;
	padding: 5px;
	color: white;
	
	
}
th{
	width: 100px;
	background: #9da5b924;
}
