/*로그인후 css*/

.logout ul {padding: 0; margin-left: -270px;}
.logout li {color: #eee; font-size: 14px;}
	
.bot {
	font-size: 12px;
    border: 1px solid #eee;
    padding: 5px 10px;
    border-radius: 4px;
    color: #eee;
    background: none;
}
.bot:hover{color: #eee; background: none;}

.scrollToggle .bot {
	background: none;
    font-size: 12px;
    border: 1px solid #3c4990;
    padding: 5px 10px;
    color: #3c4990;
    border-radius: 4px;
}
.scrollToggle .bot:hover{color: #eee; background: #3c4990;}
.logout li span {
    color: #eee;
	margin-right: 10px;
}
.scrollToggle .logout li span {
    color: #3c4990;
	margin-right: 10px;
}
.logout i {
	color: #eee;
	margin-right: 10px;
}
.scrollToggle .logout i {
	color: #3c4990;
	margin-right: 10px;
}
