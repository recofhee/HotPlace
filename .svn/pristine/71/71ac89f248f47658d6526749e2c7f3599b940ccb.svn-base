*{ margin: 0;padding: 0;box-sizing: border-box;}
#container{width: 100%; margin: 0 auto; padding: 0; height:0 auto;  border: 0px solid black;}
#container header{position:fixed; 
	   z-index: 99999; 
	   transition:.3s linear .1s; 
	   color: #222222;
	   text-align: center; 
	   width: 100%; 
	   top:-4px; 
	   height: 65px;
	   background: #00000066;}
#container nav{position:fixed; z-index: 99999; transition:.3s linear .1s; /* color: #222222; */}
/* header {top:0; height:0 auto; width: 100%; margin: auto;color:#ddd; } */
#container header h2{padding: 0px 740px 0px 0px;}
#container header h2 a{text-decoration: none;}
.mainimg a img{width: 70px; height: 50px; margin-left: -100px; margin-top: 10px;}
/* header h2:hover a{color: red;} */
#container .scrollToggle {background:rgba(255,255,255,0.7); color:gray;/* box-shadow: 0 4px 3px 0px #523b1c; */ height: 65px;}
#container .scrollToggle ul li{background: #3443b770; width: 65px;}
#container .scrollToggle ul li a {color: black;}
#container .scrollToggle li a:hover, .scrollToggle li a:focus{color: red; /* text-shadow: 0 0 5px; */ }
#container .scrollToggle .logout li {color: black; font-size: 12px; width: 400px; background: #e8e1e180;}
#container .scrollToggle1 {background:rgba(255,255,255,0.7); color:aqua; box-shadow: 0 4px 3px 0px white; }
#container .scrollToggle1 li a:hover, .scrollToggle li a:focus{color:gray /* rgba(91, 192, 222, 0.7) */; /* text-shadow: 0 0 0px; */ }
#container .scrollToggle1 li a{color: black;}
#container .scrollToggle1 ul li a:active {color: red;}
.login { margin-top: -45px; margin-left: 400px;}
.login ul{/* padding: 0px 0px 0px 410px; */ display: inline-flex; margin-left: 410px}
