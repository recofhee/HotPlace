@charset "UTF-8";
body{background: url(../images/monsterbackground.jpg);
	 background-attachment: fixed;
	 background-size: cover;}
section {padding: 15px;
		 padding-top: 150px;
		 }
.wrap{
	width: 960px;
	margin: 0 auto;
	height: auto;
	border:  0px solid black;
	background-repeat: no-repeat;
	background-size: 100% 100%;
	background: #000000c4;
	
}

.bt {
	position: relative;
	border: 1px solid white;
	background: rgb(198,47,99);
	width: 80px;
	height: 25px;
	font-size: 12px;
	color: white;
	cursor: pointer;
	margin: 5px;
}

.bt:hover {
	border: 1px solid;
	color: black;
	background: #b51f8d;
	font-weight: bold;
}
table {
	width: 700px; 
	border-collapse: collapse;
	font-size: 12px;
	line-height: 15px;
	
}
table td, th{
	border:  1px solid #90b4e63d;
	padding: 5px;
	color: white;
	
	
}
th{
	width: 100px;
	background: #9da5b924;
}
img{
	
}
a{
	text-decoration: none;
	color: black;
	
	
}
a:hover {
	text-decoration: none;
	color: red;
	text-shadow: 0 2px 2px #000;
}
.td1 td{
	border-top: 1px solid #90b4e6;
    border-bottom: 1px solid #90b4e6;
    background-color: #c3d8f5;}
    
 



.center {margin: 30px; 
		 min-height: 600px;}

.h1 {
	background: url(../images/icon/monboar.png) no-repeat 15px 20px;
    width: 430px;
    height: 85px;
    background-size: 60%;
    float: left;
}
.center h2{
   font-size: 2rem;
    text-align: -webkit-left;
    color: white;
    padding: 25px;
}
