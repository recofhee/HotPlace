@charset "UTF-8";
 body{background-image: url('../images/back.jpg');
	background-repeat:no-repeat;
    background-size: 100% 100%;
    background-attachment: fixed;
    }
*{ margin: 0;padding: 0;box-sizing: border-box;}
#container{width: 100%; margin: 0 auto; padding: 0; height:0 auto;  border: 0px solid black;}
header{position:fixed; z-index: 99999; transition:.3s linear .1s; color: #222222;text-align: center; width: 100%}
#container nav{position:fixed; z-index: 99999; transition:.3s linear .1s; color: #222222;}
/* header {top:0; height:0 auto; width: 100%; margin: auto;color:#ddd; } */
header h2{padding: 0px 740px 0px 0px;}
header h2 a{text-decoration: none;}
/* header h2:hover a{color: red;} */
.scrollToggle {background:#c9302c8f; color:gray;/* box-shadow: 0 4px 3px 0px #523b1c; */}
.scrollToggle li a:hover, .scrollToggle li a:focus{color: red; text-shadow: 0 0 5px; }
.scrollToggle1 {background:#c9302c8f;/ color:aqua; box-shadow: 0 4px 3px 0px white; }
.scrollToggle1 li a:hover, .scrollToggle li a:focus{color:gray /* rgba(91, 192, 222, 0.7) */; /* text-shadow: 0 0 0px; */ }
.scrollToggle1 li a{color: black;}
.scrollToggle1 ul li a:active {color: red;}
.login { margin-top: -45px; margin-left: 400px;}
.login ul{padding: 0px 0px 0px 410px; display: inline-flex;}
.login li{list-style: none; /* float: right; */ right:1000px; margin: 5px;}
.login li a{text-decoration: none; font-size: 14px;}
.login li:hover a{color: fuchsia;}
.login li a:active {color: red;}

