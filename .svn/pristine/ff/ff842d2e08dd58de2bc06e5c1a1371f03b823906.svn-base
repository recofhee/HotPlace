@charset "UTF-8";

section{width: 960px; border: 0px solid black; height: 0 auto; margin:auto; padding-top:65px; position: relative;}
#artcle3{width: 99%; border: 0px solid black; margin:-50px; padding:5px;padding-top:90px; display: block;  height: 540px;}
aside{width: 370px; border: 0px solid black; padding: 5px; display: block; height: 400px; float: right; margin-top: -545px;}

.box1 {
	width:100%;
	display: inline-block; 
	border: 0px solid black;
	font-size: 13px;
	box-sizing: border-box;
	text-align: left;
	
 }
.box1 li {
 	width: 100%;
	/* line-height: 20px; */
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
	}
.box1 li a{color: white; text-decoration: none;}
.box1 li:hover a{color: red;}

table {
    width: 75%;
    border-top: 1px solid #fff;
    border-collapse: collapse;
    margin-bottom: 10px;
}
th, td {
    border-bottom: 1px solid #fff;
    color: white;
}
th{
	width: auto;
}
td{
	padding-left : 5px;
	width: 100%;
}