@charset "UTF-8";
body{background: url(../images/monsterbackground.jpg);
 	 background-attachment: fixed;
	 background-size: cover;}
section {padding-top: 150px;
		}
.warp{
	width: 960px;
	margin: 30px auto;
	border:  0px solid black;
	background-repeat: no-repeat;
	background-size: cover;
	background: #000000c4;
}
.bt {
	position: relative;
	border: 1px solid white;
	background: rgb(198,47,99);
	width: 80px;
	height: 25px;
	font-size: 12px;
	color: white;
	cursor: pointer;
	margin: 5px;
}

.bt:hover {
	border: 1px solid;
	color: whtie;
	background: #b51f8d;
	font-weight: bold;
}

h1{
	color: navy;
}
table {
	border-collapse: collapse;
	font-size: 12px;
	line-height: 15px;
	
}
table td, th{
	border:  1px solid #90b4e63d;
	padding: 5px;
	
	
}
th{
	background: #9da5b924;
	color: white;
}
img{
}
a{
	text-decoration: none;
	color: black;
	}
a:hover {
	text-decoration: none;
	color: red;
	text-shadow: 0 2px 2px #000;
}
.td1 td{
