.wrap{
	width: 1040px;
	margin: 30px auto;
	border: : 1px solid black;
	background: url(../images/icon/monstergu.jpg);
	background-repeat: no-repeat;
	background-size: 100% 100%;
	opacity: .9;
}
.bt {
	position: relative;
	width: 80px;
	height: 25px;
	font-size: 12px;
	color: black;
	background: white;
	cursor: pointer;
	margin: 10px;
	}
.bt:hover{
	border:1px solid;
	color: red;
}

}
table {
	/* width: 70%;  */
	border-collapse: collapse;
	font-size: 12px;
	line-height: 15px;
	
}
table td, th{
	border:  1px solid #90b4e63d;
	padding: 5px;
	color: white;
	
	
}
th{
	background: #9da5b924;
	color: white;
}
img{
	/* width: 220px;
	height: 300px; */
}
a{
	text-decoration: none;
	color: black;
	}
a:hover {
	text-decoration: none;
	color: red;
	text-shadow: 0 2px 2px #000;
}
.td1 td{
	border-top: 1px solid #90b4e6;
